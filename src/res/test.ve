extern puti(n: int);
extern putd(n: double);

func pow(d: double, n: int): double
{
    let x:double = 1;

    for(let i=1; i<=n; ++i)
    {
        x*=d;
    }

    return x;
}

func is_munch(n: double): bool
{
    let dsum=0.0d;

    let copy=n;

    while(copy>10)
    {
        let dig=copy%10;
        copy/=10;
        dsum+=pow(dig,dig);
    }

    putd(dsum);
    putd(n);

    if(dsum==n)
    {
        return true;
    }

    return false;
}

is_munch(1375);